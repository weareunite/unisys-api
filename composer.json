{
  "name": "weareunite/unisys-api",
  "description": "Unisys basic API skeleton.",
  "keywords": [
    "unisys",
    "unisys api",
    "unite",
    "weareunite",
    "api",
    "laravel"
  ],
  "license": "MIT",
  "homepage": "https://github.com/weareunite/unisys-api",
  "authors": [
    {
      "name": "Vladimír Žilka",
      "email": "vlado.zilka@weareunite.com",
      "homepage": "https://weareunite.com",
      "role": "Developer"
    },
    {
      "name": "Michal Stupák",
      "email": "michal.stupak@weareunite.com",
      "homepage": "https://weareunite.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "laravel/framework": "5.6.*",
    "laravel/passport": "^4.0",
    "predis/predis": "^1.1",
    "spatie/laravel-permission": "^2.9",
    "spatie/laravel-backup": "^5.2",
    "spatie/laravel-activitylog": "^2.5",
    "spatie/laravel-medialibrary": "^7.0.0",
    "spatie/laravel-model-status": "^1.0.6",
    "spatie/eloquent-sortable": "^3.4",
    "barryvdh/laravel-cors": "^0.11",
    "barryvdh/laravel-snappy": "^0.4.1",
    "aws/aws-sdk-php": "~3.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "maatwebsite/excel": "^3.0|^3.1",
    "sentry/sentry-laravel": "^0.8.0",
    "barryvdh/laravel-ide-helper": "^2.3",
    "mpociot/laravel-apidoc-generator": "^2.0",
    "weareunite/unisys-contacts": "^0.1.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.3",
    "doctrine/dbal": "^2.5"
  },
  "autoload": {
    "classmap": [],
    "psr-4": {
      "Unite\\UnisysApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Unite\\UnisysApi\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Unite\\UnisysApi\\UnisysApiServiceProvider"
      ]
    }
  }
}